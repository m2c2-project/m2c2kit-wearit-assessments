{
  "name": "m2c2kit-wearit-assessments",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npm run build-assessments && npm run copy-assessments",
    "build-assessments": "concurrently \"npm run build -w packages/assessment-symbol-search-wearit\" \"npm run build -w packages/assessment-color-dots-wearit\"",
    "copy-assessments": "cpy packages/assessment-symbol-search-wearit/dist/** dist/assessments/symbol-search && cpy packages/assessment-color-dots-wearit/dist/** dist/assessments/color-dots",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "8.2.0",
    "cpy-cli": "5.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0",
    "rimraf": "5.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "workspaces": [
    "packages/assessment-symbol-search-wearit",
    "packages/assessment-color-dots-wearit"
  ],
  "dependencies": {
    "@m2c2kit/addons": "0.3.9",
    "@m2c2kit/assessment-color-dots": "0.8.10",
    "@m2c2kit/core": "0.3.11"
  }
}
